  <LifeLessons.LearningActivityDef>
    <defName>LLDissectCritterCorpse</defName>
    <label>Critter Dissection</label>
    <description>Dissecting animal and insects to understand non-human biological processes and structures.</description>
    <activityJob>LLCritterAutopsy</activityJob>
    <workstationThingGroup>BuildingArtificial</workstationThingGroup>
    <experienceType>Theoretical</experienceType>
    <xpPerTick>0.0025</xpPerTick>
    <dailyRepetitionsUntilExhausted>3</dailyRepetitionsUntilExhausted>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <validWorkstations>
      <li>LLAutopsyTable</li>
    </validWorkstations>
    <consumeIngredientTargets>true</consumeIngredientTargets>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Corpses</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Corpses</li>
      </categories>
      <disallowedCategories>
        <li>CorpsesMechanoid</li>
        <li>CorpsesHumanlike</li>
      </disallowedCategories>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <placeTargetExactly>true</placeTargetExactly>
  </LifeLessons.LearningActivityDef>

