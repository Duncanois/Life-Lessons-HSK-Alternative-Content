  <!-- Represents what might reasonably be considered common-knowledge firstaid for industrial+ society.-->
  <LifeLessons.BackstoryProficiencyTemplateDef>
    <defName>BasicFirstaid</defName>
    <applyToAnyStory>true</applyToAnyStory>
    <disablePatcherAssignment>true</disablePatcherAssignment>
    <package>
      <proficiencies MayRequire="ghostdata.lifelessons.medicalmishaps">
        <li MayRequire="ghostdata.lifelessons.medicalmishaps">SimpleWoundDressing</li>
        <li MayRequire="ghostdata.lifelessons.medicalmishaps">WoundCleaning</li>
        <li MayRequire="ghostdata.lifelessons.medicalmishaps">Sanitation</li>
      </proficiencies>
    </package>

    <heuristics>
      <li Class="LifeLessons.BackstoryHeuristic">
        <debugId>BasicFirstaid-Adult</debugId>
        <backstorySlot>Adulthood</backstorySlot>
        <and Class="LifeLessons.TechLevelHeuristic">
          <debugId>BasicFirstaid-Techlevel</debugId>
          <greaterThan>Medieval</greaterThan>
          <and Class="LifeLessons.SkillHeuristic">
            <debugId>BasicFirstaid-Skill</debugId>
            <skill>
              <skillDef>Medicine</skillDef>
              <greaterThan>2</greaterThan>
            </skill>
          </and>
        </and>
      </li>
    </heuristics>
  </LifeLessons.BackstoryProficiencyTemplateDef>

