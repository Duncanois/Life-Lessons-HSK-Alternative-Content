    <proficiencyAdjective>physical</proficiencyAdjective>
    <category>ProficiencyCategory_Physics</category>
    <description>Understanding of the physical properties, processes, and interactions of matter and energy.</description>
    <costTheoretical>1200</costTheoretical>
    <canBePioneered>true</canBePioneered>
    <effectiveSubjectLimit>2</effectiveSubjectLimit>
    <viewerX>8</viewerX>
    <viewerY>3</viewerY>
