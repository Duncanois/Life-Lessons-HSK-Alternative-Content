<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>*/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLNeurotrainerBasic</tradeTag>
				<thingDefCountRange>0~2</thingDefCountRange>
				<countRange>1~3</countRange>
				<price>Normal</price>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLNeurotrainerUncommon</tradeTag>
				<thingDefCountRange>-2~2</thingDefCountRange>
				<countRange>1~2</countRange>
				<price>Expensive</price>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLNeurotrainerRare</tradeTag>
				<thingDefCountRange>-2~1</thingDefCountRange>
				<countRange>1~1</countRange>
				<price>Expensive</price>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLTextbooksUncommon</tradeTag>
				<thingDefCountRange>0~2</thingDefCountRange>
				<countRange>1~2</countRange>
				<price>Expensive</price>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLTextbooksRare</tradeTag>
				<thingDefCountRange>-1~2</thingDefCountRange>
				<countRange>1~2</countRange>
				<price>Expensive</price>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>*/TraderKindDef[defName="Orbital_BulkGoods"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLTextbooksBasic</tradeTag>
				<thingDefCountRange>1~4</thingDefCountRange>
				<countRange>1~4</countRange>
				<price>Normal</price>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLTextbooksUncommon</tradeTag>
				<thingDefCountRange>0~2</thingDefCountRange>
				<countRange>1~2</countRange>
				<price>Normal</price>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>LLTextbooksRare</tradeTag>
				<thingDefCountRange>-2~1</thingDefCountRange>
				<countRange>1~1</countRange>
				<price>Expensive</price>
			</li>
			
		</value>


	</Operation>

</Patch>

